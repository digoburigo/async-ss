{
  "name": "@acme/web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm env:run build",
    "clean": "git clean -xdf .cache dist .turbo node_modules .tanstack dev-dist",
    "dev": "pnpm with-env vite dev",
    "env:copy-example": "shx -- cp -n .env.example .env",
    "env:remove": "shx -- rm -f .env",
    "env:run": "dotenv -- vite --configLoader runner",
    "format": "prettier --check . --ignore-path .prettierignore --ignore-path ../../.gitignore",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "preview": "pnpm env:run preview",
    "start": "pnpm env:run",
    "typecheck": "tsc --build",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@acme/prettier-config",
  "dependencies": {
    "@acme/auth": "workspace:*",
    "@acme/ui": "workspace:*",
    "@acme/zen-v3": "workspace:*",
    "@radix-ui/react-icons": "^1.3.2",
    "@tailwindcss/vite": "catalog:",
    "@tanstack/react-form": "catalog:",
    "@tanstack/react-query": "catalog:",
    "@tanstack/react-router": "^1.134.15",
    "@zenstackhq/tanstack-query": "^3.0.0-beta.19",
    "better-auth": "catalog:",
    "dotenv-cli": "^11.0.0",
    "next-themes": "^0.4.6",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "sonner": "^2.0.7",
    "tailwindcss": "catalog:",
    "vite-plugin-pwa": "^1.1.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tailwind-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@tanstack/router-devtools": "^1.134.15",
    "@tanstack/router-plugin": "1.134.15",
    "@types/node": "catalog:",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vitejs/plugin-react": "catalog:",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "catalog:",
